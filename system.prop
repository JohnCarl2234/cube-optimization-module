# This file will be read by resetprop
# Example: Change dpi
# ro.sf.lcd_density=320

# ActivityManager: Killing 9989:com.hypergryph.arknights/u0a298 (adj 950): cached op#31
# Due to activity_manager_constants.CUR_MAX_CACHED_PROCESSES = 60
# CUR_MAX_EMPTY_PROCESSES = CUR_MAX_CACHED_PROCESSES/2
# working on android 9, not 10
ro.vendor.qti.sys.fw.bg_apps_limit=600
ro.vendor.qti.sys.fw.bservice_limit=60

# disable memplus prefetcher which ram-boost relying on, use traditional swapping
persist.vendor.sys.memplus.enable=false
